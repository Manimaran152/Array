import java.util.Scanner;

class Array {
  public static void main (String []args){
      int array[];
      int size;
      System.out.println("size:");
      Scanner sc=new Scanner(System.in);
      size =sc.nextInt();
      array=new int[size];
      for (int i=0;i<size ;i++){
        array[i] = sc.nextInt();
        if(array[i]%2==0){
            System.out.println("even" );
        }
        else{
            System.out.println("odd");
      } 
    } 
  }
} 
